{
  "name": "@hebo/gateway",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "engines": {
    "bun": "^1.3.6"
  },
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --compile --minify-whitespace --minify-syntax --target=bun --outfile dist/server",
    "clean": "git clean -fdx -e '.env*' .",
    "dev": "bun --hot src/index.ts | pino-pretty",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prestart": "bun run build",
    "start": "./dist/server",
    "test": "bun test"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^3.0.73",
    "@ai-sdk/google-vertex": "^3.0.97",
    "@ai-sdk/groq": "^2.0.34",
    "@ai-sdk/provider-utils": "catalog:",
    "@aws-sdk/client-bedrock": "^3.968.0",
    "@aws-sdk/client-sts": "^3.968.0",
    "@bogeychan/elysia-logger": "catalog:",
    "@elysiajs/cors": "catalog:",
    "@elysiajs/openapi": "catalog:",
    "@elysiajs/opentelemetry": "catalog:",
    "@hebo/api": "workspace:*",
    "@hebo/shared-api": "workspace:*",
    "ai": "catalog:",
    "better-auth": "catalog:",
    "elysia": "catalog:",
    "voyage-ai-provider": "^2.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@hebo/typescript-config": "workspace:*",
    "bun-types": "catalog:",
    "pino-pretty": "catalog:",
    "typescript": "catalog:"
  }
}
