{
  "name": "hebo",
  "version": "0.1.0",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "bun-types": "catalog:",
    "check-dependency-version-consistency": "^6.0.0",
    "eslint": "^9.39.2",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-import-resolver-typescript-bun": "^0.0.104",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "eslint-plugin-turbo": "^2.7.3",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "lefthook": "^2.0.15",
    "prettier": "^3.7.4",
    "sst": "catalog:",
    "turbo": "^2.7.4",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "^8.53.0"
  },
  "engines": {
    "bun": "^1.3.6"
  },
  "packageManager": "bun@1.3.6",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -fdx -e '.env*'",
    "db:migrate": "bun run -F @hebo/api db migrate dev && bun run -F @hebo/auth db migrate dev",
    "db:reset": "bun run -F @hebo/api db migrate reset --force && bun run -F @hebo/auth db migrate reset --force",
    "dev": "turbo run dev",
    "dev:infra:up": "docker compose up -d",
    "dev:infra:down": "docker compose down",
    "format": "prettier --write .",
    "format:staged": "prettier --write",
    "lint": "turbo run lint",
    "lint:deps": "check-dependency-version-consistency .",
    "lint:staged": "eslint --cache --fix",
    "postinstall": "lefthook install",
    "secret": "bun run scripts/secrets.ts",
    "test": "turbo run test"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "catalog": {
    "ai": "^5.0.121",
    "@ai-sdk/provider-utils": "^3.0.20",
    "@ai-sdk/react": "^2.0.123",
    "@base-ui/utils": "^0.2.3",
    "@base-ui/react": "^1.0.0",
    "@bogeychan/elysia-logger": "^0.1.10",
    "@conform-to/react": "^1.15.1",
    "@conform-to/zod": "^1.15.1",
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/eden": "^1.4.3",
    "@elysiajs/openapi": "^1.4.13",
    "@elysiajs/opentelemetry": "^1.4.10",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/pg": "^8.16.0",
    "better-auth": "^1.4.12",
    "bun-types": "^1.3.6",
    "cmdk": "^1.1.1",
    "elysia": "^1.4.22",
    "lucide-react": "^0.562.0",
    "motion": "^12.26.2",
    "pino-pretty": "^13.1.1",
    "prisma": "^7.2.0",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sst": "^3.17.25",
    "streamdown": "^2.0.1",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^4.3.5"
  }
}
